<Window x:Class="MedicalCenter.eqAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MedicalCenter"
        mc:Ignorable="d"
        Title="eqAdd" Height="450" Width="800">
    <Grid>
        <TextBlock Text="Name" Margin="200,100,470,280" FontSize="24"></TextBlock>
        <TextBox x:Name="Name" Margin="350,100,200,280"></TextBox>

        <Button x:Name="ok" Content="Confirm" FontSize="20" Margin="260,309,260,54" Click="ok_Click"></Button>
        <Button x:Name="cancel" Content="Cancel" FontSize="20" Margin="657,309,10,54" Click="cancel_Click" ></Button>


    </Grid>
</Window>
