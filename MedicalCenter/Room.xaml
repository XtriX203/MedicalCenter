<Window x:Class="MedicalCenter.Room"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MedicalCenter"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Room" Height="450" Width="800">

    <Grid Margin="10">

        <TextBlock Text="Room" FontSize="30" Margin="260,50,255,300" HorizontalAlignment="Center"></TextBlock>
        <StackPanel Margin="10,0,539,300" Grid.Column="0" Orientation="Vertical" HorizontalAlignment="Center" Width="223">
            <Label HorizontalAlignment="Center" FontSize="20" Width="186">Enter room Num:</Label>
            <TextBox x:Name="num"  Width="100" Height="40" ></TextBox>
            <Button Name="okButton" Click="okButton_Click" IsDefault="True" Width="100">OK</Button>
        </StackPanel>



    </Grid>
</Window>
